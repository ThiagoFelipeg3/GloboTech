    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.7",
    "@types/node-fetch": "^2.6.2",
    "@types/redis": "^2.8.21",
    "autocannon": "^7.10.0",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
