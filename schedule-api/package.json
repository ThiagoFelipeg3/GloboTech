{
  "name": "agenda-api",
  "version": "1.0.0",
  "description": "Agenda de Jogos GE",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/index.js",
